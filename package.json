{
  "name": "graphql-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn docd run --rm --service-ports api",
    "watch": "nodemon --delay 250ms -e js,graphql -x 'kill-port 9229 && node --inspect=0.0.0.0:9229 src/index.js'",
    "start": "node src/index.js",
    "lint": "eslint src/",
    "docd": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Manjula Dube",
  "dependencies": {
    "axios": "^0.18.0",
    "fs-extra": "^7.0.0",
    "graphql-yoga": "1.16.2",
    "merge-graphql-schemas": "^1.5.7",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "kill-port": "^1.3.2",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3"
  }
}